<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows10.0.19041.0</TargetFramework>
    <TargetPlatformMinVersion>10.0.17763.0</TargetPlatformMinVersion>
	  <TargetMinVersion>10.0.17763.0</TargetMinVersion>
    <RootNamespace>ModernWindow</RootNamespace>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Platforms>x86;x64;ARM64</Platforms>
    <RuntimeIdentifiers>win-x86;win-x64;win-arm64</RuntimeIdentifiers>
    <PublishProfile>win-$(Platform).pubxml</PublishProfile>
    <UseWinUI>true</UseWinUI>
    <WindowsAppSDKWinUI>true</WindowsAppSDKWinUI>
    <EnableMsixTooling>true</EnableMsixTooling>
    <WindowsPackageType>None</WindowsPackageType>
    <StartupObject>ModernWindow.EntryPoint</StartupObject>
    <SignAssembly>False</SignAssembly>
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <AssemblyName>WingetUI</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="icon.ico" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Intereface\**" />
    <Content Remove="..\wingetui-engine\.vscode\**" />
    <EmbeddedResource Remove="Intereface\**" />
    <None Remove="Intereface\**" />
    <Page Remove="Intereface\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Interface\AboutWingetUI.xaml" />
